		$(function() {
			// slider price
			$( "#slider-price" ).slider({
				range: true,
				min: 0,
				max: 20000,
				values: [ 0, 20000 ],
				step: 500,
				vecchiovaldx: 0,
				slide: function( event, ui ) {
					$( "#amt-price-min" ).val( " " + ui.values[ 0 ] ); 
					$( "#amt-price-max" ).val( " " + ui.values[ 1 ] );
				},
				stop: function( event, ui ) {
					$( "#sform" ).submit();
				}
			});
			$( "#amt-price-min" ).val( $( "#slider-price" ).slider( "values", 0 ) + " \u20AC" );
